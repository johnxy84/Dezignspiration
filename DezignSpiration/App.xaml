<?xml version="1.0" encoding="utf-8"?>
<Application xmlns="http://xamarin.com/schemas/2014/forms" xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml" x:Class="DezignSpiration.App">
    <Application.Resources>
        <ResourceDictionary>
            <OnPlatform x:TypeArguments="x:String" x:Key="NeutonBold">
                <On Platform="Android" Value="Neuton-Bold.ttf#Neuton" />
                <On Platform="iOS" Value="Neuton-Bold" />
            </OnPlatform>
            <OnPlatform x:TypeArguments="x:String" x:Key="NeutonLight">
                <On Platform="Android" Value="Neuton-Light.ttf#Neuton" />
                <On Platform="iOS" Value="Neuton-Light" />
            </OnPlatform>
        </ResourceDictionary>
    </Application.Resources>
</Application>